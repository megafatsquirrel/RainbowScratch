{
  "name": "rainbow-scratch-comic",
  "version": "0.2.5",
  "description": "A webcomic",
  "engines": {
    "node": "8.x"
  },
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "npm run build:rainbow && npm run build:army-builder-server && npm run build:army-builder-client",
    "build:army-builder-client": "cross-env NODE_ENV=production webpack --config webpack.army.builder.config.js --progress --hide-modules",
    "build:army-builder-server": "cross-env NODE_ENV=production webpack --config webpack.army.builder.server.config.js --progress --hide-modules",
    "build:rainbow": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --hide-modules",
    "build-dev": "cross-env NODE_ENV=development webpack --progress --config webpack.config.js",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "ajv": "^6.1.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "bulma": "^0.6.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "ejs": "2.5.5",
    "express": "4.13.3",
    "express-vue-builder": "^2.15.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "helmet": "^3.8.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "migrate": "^1.2.0",
    "mongodb": "^3.0.2",
    "style-loader": "^0.19.1",
    "vue": "^2.5.13",
    "vue-builder": "^1.7.2",
    "vue-loader": "^14.1.1",
    "vue-resource": "^1.3.6",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.13",
    "vue-ssr-webpack-plugin": "^3.0.0",
    "vue-template-compiler": "^2.5.13",
    "vuex": "^3.0.1",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "repository": "https://github.com/megafatsquirrel/RainbowScratch",
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT",
  "author": "Matt English <menglish2451@gmail.com>",
  "devDependencies": {}
}
